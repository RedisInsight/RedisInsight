{
  "build": "development",
  "defaultPort": 5540,
  "host": "localhost",
  "debug": true,
  "themeSource": "dark",
  "appName": "Redis Insight",
  "schema": "redisinsight",
  "mainWindow": {
    "show": false,
    "width": 1300,
    "height": 860,
    "minHeight": 680,
    "minWidth": 960,
